web: java -Dserver.port=$PORT $JAVA_OPTS -jar target/email-service.jar --spring.rabbitmq.addresses=$CLOUDAMQP_URL --spring.mail.host=$MAIL_HOST --spring.mail.username=$MAIL_USERNAME --spring.mail.password=$MAIL_PASSWORD --spring.mail.port=$MAIL_PORT --spring.mail.properties.mail.smtp.starttls.enable=$MAIL_STARTTLS